{
  "name": "commandhelper",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "*",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "node-gyp": "^11.5.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "*",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.15",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "electron": "^31.7.5",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.24.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "vite": "6.3.5",
    "wait-on": "^8.0.0"
  },
  "scripts": {
    "dev:main": "esbuild electron/main.ts --platform=node --format=cjs --outfile=electron/main.js --watch",
    "dev:preload": "esbuild electron/preload.ts --platform=node --format=cjs --outfile=electron/preload.js --watch",
    "dev:db": "esbuild electron/db.ts --platform=node --format=cjs --outfile=electron/db.js --watch",
    "dev": "concurrently -k \"npm:dev:main\" \"npm:dev:preload\" \"npm:dev:db\" \"vite\" \"wait-on http://localhost:5173 file:electron/main.js file:electron/preload.js file:electron/db.js && VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build:renderer": "vite build",
    "build:main": "esbuild electron/main.ts --platform=node --format=cjs --outfile=electron/main.js",
    "build:preload": "esbuild electron/preload.ts --platform=node --format=cjs --outfile=electron/preload.js",
    "build:db": "esbuild electron/db.ts --platform=node --format=cjs --outfile=electron/db.js",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload && npm run build:db",
    "start": "npm run build && electron .",
    "dist": "npm run build && electron-builder --mac dmg"
  },
  "build": {
    "appId": "com.commandhelper.app",
    "productName": "Command Helper",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/*.js",
      "electron/trayTemplate.png"
    ],
    "asarUnpack": [
      "**/node_modules/better-sqlite3/**/*"
    ],
    "nodeGypRebuild": false,
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "extraResources": [
      "commands.sqlite"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ]
    }
  }
}
